{
    "name": "ncsu/gclient",
    "description": "Package to provide easy facade access to the Google Client API",
    "require": {
        "php": "^7.1.3",
        "google/apiclient": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.4"
    },
    "suggest": {
        "beyondcode/laravel-credentials": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "NCSU\\GClient\\": "src"
        }
    },
    "authors": [
        {
            "name": "Jason Maners",
            "email": "jlmaners@ncsu.edu"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "NCSU\\GClient\\GoogleClientServiceProvider"
            ],
            "aliases": {
                "GClient": "NCSU\\GClient\\Facade"
            }
        }
    },
    "minimum-stability": "dev"
}
